<?xml version="1.0" encoding="utf-8"?>
<project>
	<meta title="HXPBunnies" package="haxepunk.bunnymark" version="1.0.0" company="HaxePunk"/>

	<app file="HXPBunnies" main="Main" path="bin"/>

	<window fps="60" orientation="portrait" resizable="true" background="0x333333" hardware="true"/>
	<!--<window width="640" height="480" unless="mobile"/>-->
	<window width="1280" height="720" unless="mobile"/>
	<window fullscreen="true" if="mobile"/>

	<source path="src"/>

	<haxelib name="HaxePunk"/>

	<assets path="assets/graphics" rename="gfx" include="*.png|*.jpg" embed="true"/>
	<assets path="assets/atlas" rename="atlas" include="*.xml" embed="true"/>
</project>
